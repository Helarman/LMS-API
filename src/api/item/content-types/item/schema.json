{
  "kind": "collectionType",
  "collectionName": "items",
  "info": {
    "singularName": "item",
    "pluralName": "items",
    "displayName": "Items",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "lesson",
        "examination"
      ],
      "required": true
    },
    "elements": {
      "type": "dynamiczone",
      "components": [
        "lessons.audio",
        "lessons.text",
        "lessons.video",
        "examinations.test",
        "examinations.file"
      ]
    },
    "courses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::course.course",
      "mappedBy": "items"
    },
    "description": {
      "type": "string"
    },
    "results": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::result.result",
      "mappedBy": "item"
    }
  }
}
